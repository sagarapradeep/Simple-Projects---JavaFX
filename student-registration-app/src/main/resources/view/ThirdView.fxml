<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.dep10.app.control.ThirdViewController">
   <children>
      <VBox layoutX="22.0" layoutY="60.0" prefHeight="486.0" prefWidth="457.0" spacing="50.0" AnchorPane.leftAnchor="22.0" AnchorPane.rightAnchor="21.0">
         <children>
            <HBox spacing="20.0">
               <children>
                  <Label alignment="CENTER_RIGHT" prefWidth="110.0" text="Couse Status" />
                  <RadioButton fx:id="rdoFullTime" maxWidth="79.0" mnemonicParsing="false" prefWidth="100.0" text="full time" />
                  <RadioButton fx:id="rdoPartTime" maxWidth="127.0" mnemonicParsing="false" prefHeight="18.0" prefWidth="100.0" text="part time" />
               </children>
            </HBox>
            <HBox alignment="CENTER" VBox.vgrow="NEVER">
               <children>
                  <ComboBox fx:id="cmbDepartment" prefHeight="24.0" prefWidth="196.0" />
               </children>
            </HBox>
            <HBox spacing="15.0">
               <children>
                  <ListView fx:id="lstAllModules" prefHeight="200.0" prefWidth="200.0" />
                  <VBox alignment="CENTER" spacing="10.0">
                     <children>
                        <Button mnemonicParsing="false" text="&gt;" />
                        <Button mnemonicParsing="false" text="&lt;" />
                     </children>
                  </VBox>
                  <ListView fx:id="lstSelectedModules" prefHeight="200.0" prefWidth="200.0" />
               </children>
            </HBox>
            <HBox>
               <children>
                  <HBox prefHeight="58.0" prefWidth="202.0">
                     <children>
                        <VBox>
                           <children>
                              <Button fx:id="btnBck2" mnemonicParsing="false" onAction="#btnBck2OnAction" text="&lt;-" />
                           </children>
                        </VBox>
                     </children>
                  </HBox>
                  <HBox alignment="TOP_RIGHT">
                     <children>
                        <VBox prefHeight="58.0" prefWidth="254.0" spacing="10.0">
                           <children>
                              <Button fx:id="btnSaveModules" mnemonicParsing="false" onAction="#btnSaveModulesOnAction" prefHeight="24.0" prefWidth="110.0" text="Save Modules" />
                              <Button fx:id="btnSave" mnemonicParsing="false" onAction="#btnSaveOnAction" prefWidth="110.0" text="Save Student" />
                           </children>
                        </VBox>
                     </children>
                  </HBox>
               </children>
            </HBox>
         </children>
      </VBox>
      <Label layoutX="14.0" layoutY="14.0" prefHeight="18.0" prefWidth="156.0" text="Couse Details">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
